 \"danger\";\n  } else if (\n    combinedStatus.combinedStatus === status.WARNING ||\n    combinedStatus.carStatus === status.WARNING\n  ) {\n    variant = \"warning\";\n  }\n\n  return variant;\n};\n\nconst mapStateToProps = (state) => {\n  const combinedStatus = getCombinedStatus(state);\n  const carStatus = getCarStatus(state);\n  const vanStatus = getVanStatus(state);\n  return { combinedStatus, carStatus, vanStatus };\n};\n\nexport default connect(mapStateToProps)(SummaryStatus);\n","/Users/andrewtdev/dev/tow-safe-app/src/redux/storeSaver.js",[],"/Users/andrewtdev/dev/tow-safe-app/src/configcomponents/van/VanConfig.js",[],"/Users/andrewtdev/dev/tow-safe-app/src/configcomponents/van/ManageVanLoad.js",[],"/Users/andrewtdev/dev/tow-safe-app/src/configcomponents/car/CarConfig.js",[],"/Users/andrewtdev/dev/tow-safe-app/src/configcomponents/car/ManageCarLoad.js",[],{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","severity":1,"message":"115","line":9,"column":1,"nodeType":"116","endLine":27,"endColumn":2},{"ruleId":"117","severity":1,"message":"118","line":6,"column":17,"nodeType":"119","messageId":"120","endLine":6,"endColumn":26},{"ruleId":"114","severity":1,"message":"115","line":46,"column":1,"nodeType":"116","endLine":113,"endColumn":2},{"ruleId":"121","severity":1,"message":"122","line":101,"column":11,"nodeType":"119","messageId":"123","endLine":101,"endColumn":16},{"ruleId":"121","severity":1,"message":"124","line":102,"column":11,"nodeType":"119","messageId":"123","endLine":102,"endColumn":15},{"ruleId":"117","severity":1,"message":"125","line":1,"column":17,"nodeType":"119","messageId":"120","endLine":1,"endColumn":25},{"ruleId":"117","severity":1,"message":"118","line":8,"column":10,"nodeType":"119","messageId":"120","endLine":8,"endColumn":19},{"ruleId":"117","severity":1,"message":"126","line":87,"column":7,"nodeType":"119","messageId":"120","endLine":87,"endColumn":23},"no-native-reassign",["127"],"no-negated-in-lh