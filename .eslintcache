rt variant={getStatusVariant(carStatus)} data-testid=\"car-status-box\">\n      <Row>\n        <Col xs={\"auto\"}>\n          <Link\n            to=\"/carconfig\"\n            data-testid=\"car-config-link\"\n            style={{ color: \"inherit\" }}\n          >\n            <FaTruckPickup size=\"50\" data-testid=\"car-config\"></FaTruckPickup>\n          </Link>\n        </Col>\n        <Col>\n          <Row>\n            <Col>\n              {`${carStatus.totalWeight} (${carConfig.gvm}) - Total Weight (GVM)`}\n            </Col>\n          </Row>\n          <Row>\n            <Col>{`${carStatus.remainingPayload} - Remaining Payload`}</Col>\n          </Row>\n        </Col>\n        <Col xs={\"auto\"}>\n          <Link\n            to=\"/carload\"\n            data-testid=\"car-load-link\"\n            style={{ color: \"inherit\" }}\n          >\n            <FaSuitcase size=\"25\" data-testid=\"car-manage-load\"></FaSuitcase>\n          </Link>\n        </Col>\n      </Row>\n    </Alert>\n  );\n};\n\nconst getStatusVariant = (carStatus) => {\n  var variant;\n  switch (carStatus.status) {\n    case status.OK:\n      variant = \"success\";\n      break;\n    case status.WARNING:\n      variant = \"warning\";\n      break;\n    case status.OVER:\n      variant = \"danger\";\n      break;\n    default:\n      variant = \"secondary\";\n      break;\n  }\n\n  return variant;\n};\n\nconst mapStateToProps = (state) => {\n  const carConfig = getCarConfig(state);\n  const carStatus = getCarStatus(state);\n  return { carConfig, carStatus };\n};\n\nexport default connect(mapStateToProps)(CarStatus);\n","/Users/andrewtdev/dev/tow-safe-app/src/redux/statusConstants.js",[],"/Users/andrewtdev/dev/tow-safe-app/src/indicatorcomponents/VanStatus.js",["161"],"import { connect } from \"react-redux\";\nimport { getVanConfig } from \"./../redux/selectors\";\nimport { getVanStatus } from \"./../redux/selectors\";\n\nimport { Alert, Container, Row, Col } from \"react-bootstrap\";\n\nimp